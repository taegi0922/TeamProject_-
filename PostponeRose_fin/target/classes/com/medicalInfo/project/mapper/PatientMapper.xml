<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.medicalInfo.project.mapper.PatientMapper">
	
	<insert id="registerPatient" parameterType="com.medicalInfo.project.model.PatientDTO">
		INSERT into patient (patientName,patientEmail,patientTel) VALUES (#{patientName},#{patientEmail},#{patientTel}) 
	</insert>
	
  <!-- mapper.xml의 id와 mapper 인터페이스의 메서드 명을 맞춰야 매핑 -->
  <!-- reusltType 반환 타입을 의미 -->
  	
  </mapper>
  
  